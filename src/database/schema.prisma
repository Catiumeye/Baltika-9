generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model User {
  id                  String        @id @default(uuid()) @db.Uuid
  username            String        @db.VarChar(30)
  email               String        @db.VarChar
  password            String        @db.VarChar

  @@map("user")
}